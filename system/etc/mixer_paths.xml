    <ctl name="MultiMedia1 Mixer SLIM_4_TX" value="0" />
    <ctl name="MultiMedia1 Mixer AUX_PCM_UL_TX" value="0" />
    <ctl name="MultiMedia1 Mixer SEC_AUX_PCM_UL_TX" value="0" />
